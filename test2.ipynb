     "input": [
      "# split into train and test set\n",
      "N = int(.8 * X.shape[0])\n",
      "Xtr, ytr = X[:N,:], Y[:N,:]\n",
      "Xte, yte = X[N:,:], Y[N:,:]"
     ],
